{
  "name": "@aliheym/eslint-config",
  "type": "module",
  "version": "1.0.4",
  "packageManager": "npm@9.8.1",
  "description": "aliheym's ESLint config",
  "author": "aliheym <aliheym@gmail.com> (https://github.com/aliheym/)",
  "license": "MIT",
  "homepage": "https://github.com/aliheym/eslint-config",
  "keywords": ["eslint-config"],
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "files": [
    "src/**/*.d.ts",
    "src/**/*.js"
  ],
  "engines": {
